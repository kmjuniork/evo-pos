<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="view_move_line_form_evo" model="ir.ui.view">
            <field name="name">stock.move.line.form.evo</field>
            <field name="model">stock.move.line</field>
            <field name="inherit_id" ref="stock.view_move_line_form"/>
            <field name="arch" type="xml">
                <xpath expr="//group/group[2]" position="inside">
                    <field name="previous_qty"/>
                </xpath>
            </field>
        </record>

        <record id="view_move_line_tree_evo" model="ir.ui.view">
            <field name="name">stock.move.line.tree.evo</field>
            <field name="model">stock.move.line</field>
            <field name="inherit_id" ref="stock.view_move_line_tree"/>
            <field name="arch" type="xml">
                <field name="qty_done" position="before">
                    <field name="previous_qty"/>
                </field>
            </field>
        </record>
    </data>
</odoo>